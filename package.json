{
  "name": "release-checker",
  "version": "0.0.1",
  "description": "Check your release before publishing",
  "main": "build/lib/index.js",
  "typings": "build/lib/index.d.ts",
  "bin": {
    "release-checker": "build/bin/release-checker.js"
  },
  "files":[
    "build"
  ],
  "scripts": {
    "jest--init": "jest --init",
    "test": "npm run build && npm run package && jest --config=jest.config.js",
    "test-with-no-coverage-check": "npm run build && npm run package && jest --config=jest.config.no-coverage.js",
    "test--watch": "npm run test -- --watch",
    "tsc": " tsc --listEmittedFiles",
    "tsc--init": "tsc --init",
    "build": "rimraf build && tsc",
    "release-checker": "node ./build/bin/release-checker",
    "package": "rimraf ./package && rm -f release-checker*.tgz && npm pack",
    "rimraf": "rimraf"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hdorgeval/release-checker.git"
  },
  "author": "Henri d'Orgeval",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hdorgeval/release-checker/issues"
  },
  "homepage": "https://github.com/hdorgeval/release-checker#readme",
  "devDependencies": {
    "@types/jest": "23.3.10",
    "jest": "23.6.0",
    "rimraf": "2.6.2",
    "ts-jest": "23.10.5",
    "typescript": "3.2.2"
  },
  "dependencies": {
    "@types/minimist": "1.2.0",
    "@types/node": "10.12.18",
    "minimist": "1.2.0"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
